  "phpcs.executablePath": "bin/phpcs",
  "phpunit.php": "/usr/bin/php",
  "phpunit.phpunit": "bin/phpunit",
  "php-cs-fixer.ignorePHPVersion": true,
  "php-cs-fixer.executablePath": "/home/cybot/projects/timetracker/bin/php-cs-fixer",
